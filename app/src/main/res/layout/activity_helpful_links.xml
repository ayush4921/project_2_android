<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".HelpfulLinks">

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/cznmcy1wcml2yxrll3jhd3bpegvsx2ltywdlcy93zwjzaxrlx2nvbnrlbnqvbhivcm00otqtymctmda0yy14lmpwzw_jpg" />

    <TextView
        android:id="@+id/textViewHelpfulLinks"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text="Helpful Links"
        android:textColor="#2196F3"
        android:textSize="34sp"
        android:textStyle="bold|italic"
        app:layout_constraintTop_toTopOf="@+id/imageView4"
        tools:layout_editor_absoluteX="107dp" />

    <Button
        android:id="@+id/button988"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="988 Suicide & Crisis Lifeline"
        tools:layout_editor_absoluteX="92dp"
        tools:layout_editor_absoluteY="122dp" />

    <Button
        android:id="@+id/buttonFindTreatment"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Find Treatment"
        tools:layout_editor_absoluteX="132dp"
        tools:layout_editor_absoluteY="202dp" />

    <Button
        android:id="@+id/buttonCaringForMentalHealth"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Caring for your mental health"
        tools:layout_editor_absoluteX="92dp"
        tools:layout_editor_absoluteY="287dp" />

    <Button
        android:id="@+id/button4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Button"
        tools:layout_editor_absoluteX="161dp"
        tools:layout_editor_absoluteY="387dp" />
</androidx.constraintlayout.widget.ConstraintLayout>